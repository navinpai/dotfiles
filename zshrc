# aliases
alias x="exit"
alias rm='rm -i'

alias gcm="git checkout master"
alias gdom="git diff origin/master"
alias gdomn="git diff origin/master -- name-only"
alias gc="git checkout"
alias gb="git branch"
alias gp="git pull"
alias gl="git log"
alias grh="git reset --hard HEAD"
alias gmm="git merge master"
alias gs="git status"
